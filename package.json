    "url": "https://github.com/Rakeshpradhan1999/dealCart2/issues"
  },
  "homepage": "https://github.com/Rakeshpradhan1999/dealCart2#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chart.js": "^2.9.4",
    "cloudinary": "^1.25.1",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "react-chartjs-2": "^2.11.1",
